echo "< AUTOGENERATED ICONS -->" > icons_list.xml\
for icon_file in icons/*.png; do\
    package_name=$(basename "$icon_file" .png)\
    echo "    <icon package=\"$package_name\" src=\"icons/$package_name.png\" />" >> icons_list.xml\
done
{\
  echo "< AUTOGENERATED ICONS -->"\
  for icon_file in icons/*.png; do\
    package_name=$(basename "$icon_file" .png | sed 's/[^a-zA-Z0-9._-]/_/g')\
    printf '    <icon package="%s" src="icons/%s.png" />\n' "$package_name" "$package_name"\
  done\
} > icons_list.xml
echo "< AUTOGENERATED ICONS -->" > icons_list.xml
echo "\<\!-- AUTOGENERATED ICONS --\>" > icons_list.xml
find icons -type f -name "*.png" | while read -r icon_file; do\
    package_name=$(basename "$icon_file" .png | tr -cd '[:alnum:]._-')\
    echo "    <icon package=\"$package_name\" src=\"icons/${package_name}.png\" />" >> icons_list.xml\
done
sed -i '' '/<\/theme>/r icons_list.xml' description.xml
{\
  echo '<!-- AUTOGENERATED ICONS -->'\
  find icons -type f -name "*.png" -print0 | while IFS= read -r -d '' icon_file; do\
    package_name=$(basename "$icon_file" .png | tr -cd '[:alnum:]._-')\
    printf '    <icon package="%s" src="icons/%s.png" />\n' "$package_name" "$package_name"\
  done\
} > icons_list.xml
