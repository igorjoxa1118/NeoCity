sed -i '/is_block_active()/,/^}/c\\
is_block_active() {\\
    # –Éø—Ä–æ–≤–µ—Ä–∫–∞ –≤–∫–ª—ÉÆ—Éß–µ–Ω–∏—ÉØ\\
    local enabled=$(uci -q get yandex_block.@settings[0].enabled)\\
    echo "Debug: enabled=$enabled" >&2\\
    [ "$enabled" != "1" ] && return 1\\
\\
    # –Éø–æ–ª—É£—Éß–∞–µ–º –ø–∞—Ä–∞–º–µ—Ç—Ä—É´ –≤—Ä–µ–º–µ–Ω–∏\\
    local start_h=$(uci -q get yandex_block.@settings[0].start_time)\\
    local start_m=$(uci -q get yandex_block.@settings[0].start_min)\\
    local end_h=$(uci -q get yandex_block.@settings[0].end_time)\\
    local end_m=$(uci -q get yandex_block.@settings[0].end_min)\\
    echo "Debug: raw times - start=$start_h:$start_m, end=$end_h:$end_m" >&2\\
\\
    # –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –∑–Ω–∞—Éß–µ–Ω–∏–π –ø–æ —É£–º–æ–ª—Éß–∞–Ω–∏—ÉÆ –µ—Å–ª–∏ –ø—É£—Å—Ç—É´–µ\\
    [ -z "$start_h" ] && start_h=21\\
    [ -z "$start_m" ] && start_m=30\\
    [ -z "$end_h" ] && end_h=7\\
    [ -z "$end_m" ] && end_m=0\\
    echo "Debug: processed times - start=$start_h:$start_m, end=$end_h:$end_m" >&2\\
\\
    # –ÉÇ–µ–∫—É£—É©–µ–µ –≤—Ä–µ–º—ÉØ\\
    local current_h=$(date +%-H)\\
    local current_m=$(date +%-M)\\
    local current_total=$((current_h * 60 + current_m))\\
\\
    # –É≥—Ä–∞–Ω–∏—É¶—É´ –±–ª–æ–∫–∏—Ä–æ–≤–∫–∏\\
    local start_total=$((start_h * 60 + start_m))\\
    local end_total=$((end_h * 60 + end_m))\\
    echo "Debug: totals - start=$start_total, end=$end_total, current=$current_total" >&2\\
\\
    # –Éø—Ä–æ–≤–µ—Ä–∫–∞ –ø–µ—Ä–∏–æ–¥–∞\\
    if [ $start_total -lt $end_total ]; then\\
        echo "Debug: day mode (no midnight crossing)" >&2\\
        [ $current_total -ge $start_total ] && [ $current_total -lt $end_total ] && {\\
            echo "Debug: ACTIVE (day mode)" >&2\\
            return 0\\
        }\\
    else\\
        echo "Debug: night mode (midnight crossing)" >&2\\
        [ $current_total -ge $start_total ] || [ $current_total -lt $end_total ] && {\\
            echo "Debug: ACTIVE (night mode)" >&2\\
            return 0\\
        }\\
    fi\\
\\
    echo "Debug: INACTIVE" >&2\\
    return 1\\
}' ./etc/init.d/yandex_block
